<div class="prs_mc_slider_main_wrapper">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="prs_mcc_right_side_wrapper">
          <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
              <div class="prs_mcc_right_side_heading_wrapper">
                <h2>Các phim tại rạp <%= cinema.name || "Touch Cinema" %></h2>
              </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
              <div class="tab-content">
                <div id="grid" class="tab-pane fade in active">
                  <div class="row">
                    <% cinema.Movies.forEach(function(movie) { %>
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-6 prs_upcom_slide_first">
                      <div class="prs_upcom_movie_box_wrapper prs_mcc_movie_box_wrapper">
                        <div class="prs_upcom_movie_img_box">
                          <img src="/public/images/content/<%= movie.poster %>" alt="movie_img" />
                          <div class="prs_upcom_movie_img_overlay"></div>
                          <div class="prs_upcom_movie_img_btn_wrapper">
                            <ul>
                              <li>
                                <a class="venobox" data-autoplay="true" data-vbtype="video" href="<%= movie.trailer %>">Xem Trailer</a>
                              </li>
                              <li>
                                <a href="/detail/<%= movie.unsignedName %>">Xem chi tiết</a>
                              </li>
                            </ul>
                          </div>
                        </div>
                        <div class="prs_upcom_movie_content_box">
                          <div class="prs_upcom_movie_content_box_inner">
                            <h2 class="ellipsis"><a href="/detail/<%= movie.unsignedName %>"><%= movie.name %></a></h2>
                          </div>
                          <div class="prs_upcom_movie_content_box_inner_icon">
                            <ul>
                              <li>
                                <!-- <a href="#" data-toggle="modal" data-target="#modalBook" target="_blank"><i class="flaticon-cart-of-ecommerce"></i></a> -->
                                <a href="#" data-value="<%= movie.id %>" class="btn-booking" ><i class="flaticon-cart-of-ecommerce"></i></a>
                              </li>
                            </ul>
                          </div>
                        </div>
                      </div>
                    </div>
                    <% }) %>
                    <!-- <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                      <div class="pager_wrapper gc_blog_pagination">
                        <ul class="pagination">
                          <li><a href="#"><i class="flaticon-left-arrow"></i></a>
                          </li>
                          <li><a href="#">1</a>
                          </li>
                          <li><a href="#">2</a>
                          </li>
                          <li class="prs_third_page"><a href="#">3</a>
                          </li>
                          <li class="hidden-xs"><a href="#">4</a>
                          </li>
                          <li><a href="#"><i class="flaticon-right-arrow"></i></a>
                          </li>
                        </ul>
                      </div>
                    </div> -->
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
      <div class="prs_heading_section_wrapper">
        <h2 style="padding-top: 30px;">Hình ảnh của <%= cinema.name %></h2>
      </div>
    </div>
    <div class="portfolio-area ptb-100">
      <div class="container">
        <div class="row three-column">
          <div id="gridWrapper" class="clearfix">
            <% cinema.CinemaPhotos.forEach(function(photo) { %>
            <div class="col-xs-12 col-sm-6 col-md-4 portfolio-wrapper III_column" data-groups='["all", "website", "logo"]'>
              <div class="portfolio-thumb">
                <div class="gc_filter_cont_overlay_wrapper prs_ms_scene_slider_img">
                  <img src="/public/images/content/<%= photo.name %>" class="zoom image img-responsive" alt="service_img" />
                  <div class="prs_ms_scene_img_overlay">	<a href="/public/images/content/<%= photo.name %>" class="venobox info" data-title="PORTFOLIO TITTLE" data-gall="gall12"><i class="flaticon-tool"></i></a>
                  </div>
                </div>
              </div>
              <!-- /.portfolio-thumb -->
            </div>
            <% }) %>
          </div>
          <!--/#gridWrapper-->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container -->
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
      <div class="prs_heading_section_wrapper">
        <h2 style="margin: 30px 0px;">Địa chỉ</h2>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="row">
          <div class="map">
            <!-- <p style="padding: 20px 0;"><b>Địa chỉ:</b> <%= cinema.address %></p> <br> -->
            <iframe
              width="100%"
              height="450"
              style="border:0"
              loading="lazy"
              allowfullscreen
              src="https://www.google.com/maps/embed/v1/place?key=AIzaSyAgYGUKWYXGq-QtUZWyHNQq1CvNo5CMSss
                &q=<%= cinema.address %>">
            </iframe>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%- contentFor('titlePage') %>
  Rạp <%= cinema.name %>
<%- contentFor() %>
