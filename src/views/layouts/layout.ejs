	<script>
		// $(".cinema-item").on("change", function() {
		// // Does some stuff and logs the event to the console
		//   alert($("#cinema-item").val())
		
		// });
	
		$("#cinema-item").on("change", function() {
			const chk = $("#cinema-item").val();
			if(chk === "-1") $('.form-search').get(0).setAttribute('action', '/cinema');
			else $('.form-search').get(0).setAttribute('action', `/cinema/${chk}`);
		});



		$(".btn-booking").on("click", function(e) {
			e.preventDefault();
			const movieId = $(this).data("value");
			$.ajax({
				url: "/movie/showtime",
				method: "POST",
				data: {
					movieId,
				},
				success: function (res) {
					$("#modalBook").replaceWith(res);
					$("#modalBook").modal('show');
				},
			});
		});

		$(document).on("click", ".date-showtime", function(e) {
			e.preventDefault();
			const date = $(this).data("value");
			const movieId = $(".showtime").data("value");
			$.ajax({
				url: "/movie/showtimeDate",
				method: "POST",
				data: {
					movieId,
					date,
				},
				success: function (res) {
					$("#home").replaceWith(res);
				},
			});
		});
	</script>
